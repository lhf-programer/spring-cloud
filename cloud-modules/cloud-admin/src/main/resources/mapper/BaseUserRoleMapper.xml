<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lvhaifeng.cloud.admin.mapper.BaseUserRoleMapper">

    <resultMap type="com.lvhaifeng.cloud.admin.entity.BaseUserRole" id="baseUserRoleMap">
        <result property="roleId" column="role_id"/>
        <result property="userId" column="user_id"/>
        <result property="crtTime" column="crt_time"/>
        <result property="updTime" column="upd_time"/>
    </resultMap>

    <delete id="delRoleUserByUserIds" parameterType="list">
        delete from base_user_role
        where user_id in
        <foreach collection="userIdList" open="(" separator="," close=")" item="userId">
            #{userId}
        </foreach>
    </delete>
</mapper>
