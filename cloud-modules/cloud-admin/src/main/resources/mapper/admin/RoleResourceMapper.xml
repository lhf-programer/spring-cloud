<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lvhaifeng.cloud.admin.mapper.RoleResourceMapper">

    <sql id="Base_Insert_Column_List">
        id, role_id, resource_id, type, description, crt_user, crt_time, upd_user, upd_time
    </sql>

    <!--批量插入-->
    <insert id="insertBatch">
        insert into role_resource ( <include refid="Base_Insert_Column_List" /> )
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (
            #{item.id},
            #{item.roleId},
            #{item.resourceId},
            #{item.type},
            #{item.description},
            #{item.crtUser},
            #{item.crtTime},
            #{item.updUser},
            #{item.updTime}
            )
        </foreach>
    </insert>
</mapper>